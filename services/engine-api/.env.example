PORT=8080
SUPABASE_URL=https://your-project-ref.supabase.co
# For projects using HS256 JWT signing (recommended for local dev verification).
# If set, the engine will verify access tokens using this shared secret instead of JWKS.
SUPABASE_JWT_SECRET=replace-me
SUPABASE_JWKS_URL=https://your-project-ref.supabase.co/auth/v1/.well-known/jwks.json
SUPABASE_SERVICE_ROLE_KEY=replace-me
ENGINE_INSECURE_JWT=false
ENGINE_AUTH_DEBUG=false
SUPABASE_MAX_RETRIES=2
SUPABASE_RETRY_BACKOFF_MS=300
SUPABASE_REQUEST_TIMEOUT_MS=5000
ENGINE_CACHE_TTL_SECONDS=3600
SENTRY_DSN=
OPENAI_API_KEY=
OPENAI_MODEL=gpt-5-mini
OPENAI_API_BASE_URL=https://api.openai.com
OPENAI_REQUEST_TIMEOUT_MS=60000
